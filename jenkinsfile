node {
   
   stage('SCM Checkout') { // for display purposes
      // Get some code from a GitHub repository
      git 'https://github.com/vishal2082/maven.git'
     
   }
   
   stage('Maven Build') {
    echo 'Build is started'
      withMaven(jdk: 'JDK 1.8.0_161', maven: 'Maven-3.3.9') {
         sh 'mvn clean compile  '
      }
      
   }
   
    stage('Test Execution') {
    echo 'Test is executed' 
      withMaven(jdk: 'JDK 1.8.0_161', maven: 'Maven-3.3.9') {
         sh 'mvn test package'
      }
   }
   
   stage('Archival') {
    // some block
   }
   
    stage('Deploy to Dev') {
    echo 'Deploy to Dev' 
   }
}